{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "ImagesOfResearch": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "DateOfSubmitting": {
      "type": "datetime"
    },
    "DateofEdition": {
      "type": "date"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "Description_Of_The_Research": {
      "type": "text"
    },
    "Bbody": {
      "type": "text"
    },
    "UserName": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Type": {
      "type": "string"
    },
    "isPublished": {
      "type": "boolean",
      "default": false
    },
    "IsSent": {
      "type": "boolean",
      "default": false
    },
    "NumberofLikes": {
      "type": "biginteger",
      "default": "0"
    },
    "NumberofDisLikes": {
      "type": "biginteger",
      "default": "0"
    },
    "Reviewers": {
      "type": "string",
      "default": "None"
    },
    "ReviewApproved": {
      "type": "boolean",
      "default": false
    },
    "UsersLikedit": {
      "type": "text",
      "default": "None"
    }
  }
}
